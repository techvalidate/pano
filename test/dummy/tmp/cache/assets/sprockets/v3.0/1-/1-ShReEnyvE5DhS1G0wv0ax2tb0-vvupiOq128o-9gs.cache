I"»(function() {
  $.behavior = function(target, refineSelector) {
    if (refineSelector) {
      return $('[data-behavior~=' + target + ']' + ':' + refineSelector);
    } else {
      return $('[data-behavior~=' + target + ']');
    }
  };

  $.fn.disable = function() {
    return this.each(function() {
      return $(this).prop('disabled', true);
    });
  };

  $.fn.enable = function() {
    return this.each(function() {
      return $(this).prop('disabled', false);
    });
  };

  $.fn.exists = function() {
    return this.length > 0;
  };

  $.fn.isBlank = function() {
    return this.val() === '';
  };

  $.registerBehavior = function(dataBehavior, event, callback) {
    callback();
    return $.behavior(dataBehavior).on(event, function() {
      return callback();
    });
  };

  $.fn.uncheck = function() {
    return this.each(function() {
      return $(this).prop('checked', false).removeClass('checked');
    });
  };

}).call(this);
